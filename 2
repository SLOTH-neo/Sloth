local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- é˜²åµæ¸¬ï¼ˆä¿ç•™ï¼‰
task.spawn(function()
    while task.wait(1) do
        for _, n in ipairs({"Dex", "Explorer", "Spy", "RemoteSpy"}) do
            if CoreGui:FindFirstChild(n) or (LocalPlayer.PlayerGui and LocalPlayer.PlayerGui:FindFirstChild(n)) then
                LocalPlayer:Kick("Security Alert")
                return
            end
        end
    end
end)

-- è§£å¯†å‡½æ•¸
local function dC(t)
    local r = ""
    for i = 1, #t do
        r = r .. string.char(t[i])
    end
    return r
end

-- éš±è—é€£çµï¼ˆæ•¸å­—é™£åˆ—ï¼‰
local function gU()
    local s1 = {104, 116, 116, 112, 115, 58, 47, 47} -- https://
    local s2 = {114, 97, 119, 46, 103, 105, 116, 104, 117, 98, 117, 115, 101, 114, 99, 111, 110, 116, 101, 110, 116, 46, 99, 111, 109} -- raw.githubusercontent.com
    local s3 = {47, 110, 101, 107, 111, 111, 111, 99, 97, 116, 116, 116, 45, 78, 69, 75, 79} -- /nekooocattt-NEKO
    local s4 = {47, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88, 88} -- é€™è£¡æ›æˆä½ çš„çœŸå¯¦ repo è·¯å¾‘æ•¸å­—
    local s5 = {47, 114, 101, 102, 115, 47, 104, 101, 97, 100, 115, 47, 109, 97, 105, 110} -- /refs/heads/main
    local s6 = {47, 37, 69, 56, 37, 66, 50, 37, 57, 51, 37, 69, 56, 37, 56, 53, 37, 66, 51, 37, 69, 38, 37, 57, 67, 37, 69, 38, 37, 57, 67, 37, 69, 38, 37, 57, 66, 37, 69, 38, 37, 57, 67, 37, 69, 38, 37, 57, 66, 37, 69, 38, 37, 57, 67, 37, 69, 38, 37, 57, 66, 37, 69, 38, 37, 57, 67} -- /%E8%B2%93%E8%85%B3%E6%9C%AC
    return dC(s1) .. dC(s2) .. dC(s3) .. dC(s4) .. dC(s5) .. dC(s6)
end

-- UI è¦–çª—
local sg = Instance.new("ScreenGui")
sg.Name = "NekoLauncher"
sg.Parent = CoreGui

local frame = Instance.new("Frame", sg)
frame.Size = UDim2.new(0, 340, 0, 180)
frame.Position = UDim2.new(0.5, -170, 0.5, -90)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true

Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 16)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0, 50)
title.BackgroundTransparency = 1
title.Text = "ğŸ¾ ç¢ºèªé–‹å•Ÿ Neko è…³æœ¬ï¼Ÿ"
title.TextColor3 = Color3.fromRGB(255, 150, 200)
title.Font = Enum.Font.GothamBlack
title.TextSize = 22
title.Parent = frame

local executeBtn = Instance.new("TextButton", frame)
executeBtn.Size = UDim2.new(0.8, 0, 0, 50)
executeBtn.Position = UDim2.new(0.1, 0, 0.35, 0)
executeBtn.Text = "ç«‹åˆ»å•Ÿå‹•å–µï½"
executeBtn.BackgroundColor3 = Color3.fromRGB(80, 180, 120)
executeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
executeBtn.Font = Enum.Font.GothamBold
executeBtn.TextSize = 18
Instance.new("UICorner", executeBtn).CornerRadius = UDim.new(0, 10)

local cancelBtn = Instance.new("TextButton", frame)
cancelBtn.Size = UDim2.new(0.8, 0, 0, 40)
cancelBtn.Position = UDim2.new(0.1, 0, 0.65, 0)
cancelBtn.Text = "å–æ¶ˆ"
cancelBtn.BackgroundColor3 = Color3.fromRGB(180, 80, 80)
cancelBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
cancelBtn.Font = Enum.Font.Gotham
cancelBtn.TextSize = 16
Instance.new("UICorner", cancelBtn).CornerRadius = UDim.new(0, 10)

-- è¼‰å…¥é‚è¼¯
executeBtn.MouseButton1Click:Connect(function()
    executeBtn.Text = "è¼‰å…¥ä¸­..."
    executeBtn.Active = false
    cancelBtn.Visible = false

    local success, content = pcall(function()
        return game:HttpGet(gU())
    end)

    if success and content and #content > 100 then
        sg:Destroy()
        loadstring(content)()
    else
        executeBtn.Text = "è¼‰å…¥å¤±æ•—ï¼Œå†è©¦ä¸€æ¬¡ï¼Ÿ"
        executeBtn.Active = true
        cancelBtn.Visible = true
        StarterGui:SetCore("SendNotification", {
            Title = "å–µå–µï¼Ÿ",
            Text = "è¼‰å…¥å‡ºéŒ¯äº†ï½ç¶²è·¯æˆ–é€£çµå•é¡Œï¼Ÿ",
            Duration = 5
        })
    end
end)

cancelBtn.MouseButton1Click:Connect(function()
    sg:Destroy()
end)

