-- 白名單（Username）
local Whitelist = {
    ["jay04_23"] = true,
    -- ["其他玩家名字"] = true,  -- 加信任的人
}

local localPlayer = game.Players.LocalPlayer

if not Whitelist[localPlayer.Name] then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "貓腳本",
        Text = "你不在白名單內！腳本拒絕執行喵～",
        Duration = 10
    })
    return  -- 直接踢掉，不執行
end

-- 白名單通過才載入正式腳本
loadstring(game:HttpGet("https://raw.githubusercontent.com/SLOTH-neo/Sloth/refs/heads/main/NEKO"))()
